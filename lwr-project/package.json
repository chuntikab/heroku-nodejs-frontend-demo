  "dependencies": {
    "lwr": "0.10.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "volta": {
    "node": "18.16.1"
  },
  "devDependencies": {
    "@lwc/rollup-plugin": "^3.1.3",
    "@rollup/plugin-replace": "^5.0.2",
    "lwc": "^3.1.3",
    "rollup": "^2.79.1"
  }
}
