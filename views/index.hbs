<h1>{{title}}</h1>
<div class="c-report">
	<body>
		<h1><%= title %></h1>
		<p>Welcome to <%= title %></p>
	</body>
	<form action="/result" method="post">
		{{!-- <label for="Name">Name:</label>
		<input type="text" id="Name" name="Name"><br><br> --}}
		<label for="FirstName">First name:</label>
		<input type="text" id="FirstName" name="FirstName"><br><br>
		<label for="LastName">Last name:</label>
		<input type="text" id="LastName" name="LastName"><br><br>
		<label for="Company">Company:</label>
		<input type="text" id="Company" name="Company"><br><br>
		<label for="MobilePhone">MobilePhone:</label>
		<input type="text" id="MobilePhone" name="MobilePhone"><br><br>
		<input type="submit" value="Submit">
	</form>

	<div class="c-report__content">
		<header class="c-report__header">
			<h2>{{reviewerName}}</h2>
			<date class="c-report__date">
				{{reviewDate}}
			</date>
		</header>
		<div class="c-report__text">
			<div class="c-report__section">
				<h3 class="c-report__section-title">Feedback for author(s)</h3>
				<p>{{{authorFeedbackText}}}</p>
			</div>
			<div class="c-report__section">
				<h3 class="c-report__section-title">Feedback for editor</h3>
				<p>{{{editorFeedbackText}}}</p>
			</div>
		</div>
	</div>
	<div>

	</div>
</div>